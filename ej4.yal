	let delim = [' ''\t''\n']
 let ws = delim+
let letter = ['A'-'Z''a'-'z'] 

let digit = ['0123456789']
   let id = letter (letter | digit)*

rule tokens =
    ws			{return NONE}

	| id			{return id}         (* Cambie por una acción válida, que devuelva el token *)

	| 'abc+d'					{ return PLUS }
  	| '*'				{ return TIMES }
  	| '('				{ return LPAREN }
  	| ')'				{ return RPAREN }
	| '*)'				{ return STARTCOMMENT }

